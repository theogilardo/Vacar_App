<%= simple_form_for(@request) do |f| %>
  <div class="form-group">
    <%= f.input :car_id, as: :hidden, input_html: {value: @car.id}  %>
    <%= date_field(:search, :start_date) %>
    <%= f.button :submit, {class: "btn btn-primary"}%>
  </div>
<% end %>

